@model CMCS.Web.Models.Claim

@{
    ViewData["Title"] = "Claim Details";
}

<h1>Claim Details</h1>

<div>
    <h4>Claim</h4>
    <hr />
    <dl class="row">
        <dt class="col-sm-2">Month</dt>
        <dd class="col-sm-10">@Model.PeriodMonth</dd>
        <dt class="col-sm-2">Year</dt>
        <dd class="col-sm-10">@Model.PeriodYear</dd>
        <dt class="col-sm-2">Total Amount</dt>
        <dd class="col-sm-10">@Model.TotalAmount</dd>
        <dt class="col-sm-2">Status</dt>
        <dd class="col-sm-10">@Model.Status</dd>
    </dl>
</div>

